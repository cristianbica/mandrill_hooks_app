<table border="1" cellpadding="2" cellspacing="1">
  <thead>
    <tr>
      <th>Category</th>
      <th>Sent</th>
      <th>Opened</th>
      <th>Clicked</th>
    </tr>
  </thead>
  <tbody>
<% @data.each do |row| %>
    <tr>
      <td align="left"><%= row[0] %></td>
      <td align="center"><%= row[1] %></td>
      <td align="center"><%= row[2] %></td>
      <td align="center"><%= row[3] %></td>
    </tr>
<% end %>
  </tbody>
</table>
